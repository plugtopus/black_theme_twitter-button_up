{
    "author": "Plugtopus Agency",
    "background": {
        "scripts": ["js/tnm-bg.js"]
    },
    "content_scripts": [{
        "all_frames": true,
        "js": ["js/tnm.js", "js/jquery-1.10.min.js", "js/up.js"],
        "matches": ["https://twitter.com/*", "https://twitter.com/", "https://twitter.com/i/cards/*"],
        "run_at": "document_start"
    }],
    "default_locale": "ru",
    "description": "__MSG_desc__",
    "homepage_url": "https://plugtopus.agency",
    "icons": {
        "128": "images/icon128.png",
        "16": "images/icon16.png",
        "32": "images/icon32.png"
    },
    "manifest_version": 2,
    "name": "__MSG_name__",
    "page_action": {
        "default_icon": "images/icon32.png"
    },
    "permissions": ["https://twitter.com/", "tabs", "storage"],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "20.3.18.1",
    "web_accessible_resources": ["css/style.css"]
}